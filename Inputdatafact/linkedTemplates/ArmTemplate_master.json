{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Inputdatafact"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://projectdatalake.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "a4431f4b-c207-4733-9530-34c08a9b2b8d"
		},
		"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "e0765ba1-a1e1-469a-9c8b-77e45dd84c18"
		},
		"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "assignment"
		},
		"AzureDataLakeStore2_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://projectdatalake.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore2_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "a4431f4b-c207-4733-9530-34c08a9b2b8d"
		},
		"AzureDataLakeStore2_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "e0765ba1-a1e1-469a-9c8b-77e45dd84c18"
		},
		"AzureDataLakeStore2_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "assignment"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Inputdatafact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_resourceGroupName')]"
					},
					"AzureDataLakeStore2_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore2_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore2_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore2_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore2_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore2_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore2_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore2_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}